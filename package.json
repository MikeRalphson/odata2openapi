  "bugs": {
    "url": "https://github.com/akorchev/odata2openapi/issues"
  },
  "homepage": "https://github.com/akorchev/odata2openapi#readme",
  "devDependencies": {
    "typescript": "^1.8.10",
    "typings": "^1.3.2"
  }
}
